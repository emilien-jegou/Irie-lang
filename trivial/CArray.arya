data CArray ty = CArray (#!ptr ty)
mkArray : Int -> (CArray ty)
mkArray count = CArray (alloc (count * 4))

(!) : (CArray ty) -> Int -> ty = #!extractVal
(=!) : (CArray ty) -> Int -> ty -> (CArray ty) = #!insertVal

