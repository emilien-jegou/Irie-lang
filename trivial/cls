type Int = #! Int 32
type Float = #! Float
class Num a where {
  (+) : a->a->a
--(-) : a->a->a
  }

-- Int subsumes Num
instance Num Int where {
  (+) : Int -> Int -> Int = #!add
--(-) : Int -> Int -> Int = #!sub
  }

-- Float subsumes Num
instance Num Float where {
  (+) : Float -> Float -> Float = #!fadd
--(-) : Float -> Float -> Float = #!fsub
}
default Num Int

f = 3.2 + 2.0
