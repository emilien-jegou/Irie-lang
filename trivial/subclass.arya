type Int    = #!Int 32
type Double = #!Double
type CharPtr = #!CharPtr

class Num where
  (+) : Num->Num->Num

class Real <: Num where

instance Num Int where
  (+) : Int->Int->Int = #!add
instance Real Double where
  (+) : Double->Double->Double = #!fadd

externVarArg printf : CharPtr -> Int

putN : Int -> Int
putN x = printf "%d\n" x

putFloat : Double ->  Int
putFloat f = printf "%f\n" f

main = putFloat (3+2)
